{
  "expo": {
    "name": "expo-alternate-app-icons-example",
    "slug": "expo-alternate-app-icons-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.alternateappicons.example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "expo.modules.alternateappicons.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "../app.plugin.js",
        [
          {
            "name": "red",
            "icon": "./assets/icon_red.png",
            "backgroundColor": "#ff0000",
            "paddingRound": 32
          },
          {
            "name": "green",
            "icon": "./assets/icon_green.png",
            "backgroundColor": "#00ff00"
          },
          {
            "name": "blue",
            "icon": "./assets/icon_blue.png",
            "backgroundColor": "#0000ff"
          }
        ]
      ]
    ]
  }
}